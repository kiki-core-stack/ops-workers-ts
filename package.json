{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run build:internal --outdir=./dist",
    "build:internal": "rimraf ./dist && cross-env NODE_ENV=production bun --env-file=./.env.production.local build ./src/index.ts --minify --production --target bun",
    "compile": "bun run build:internal --compile --outfile ./dist/index",
    "depcheck": "depcheck",
    "dev": "cross-env NODE_ENV=development bun --env-file=./.env.development.local --watch run ./src/index.ts",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix --max-warnings 0",
    "typecheck": "tsc",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "@kiki-core-stack/pack": "^0.89.3",
    "@kikiutils/mongoose": "^6.2.0",
    "@kikiutils/shared": "^13.5.2",
    "consola": "^3.4.2",
    "mongoose": "^8.20.0",
    "nodemailer": "^7.0.10",
    "p-mutex": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@kikiutils/eslint-config": "^5.0.0",
    "@kikiutils/tsconfigs": "^5.0.5",
    "@types/bun": "^1.3.2",
    "@types/nodemailer": "^7.0.3",
    "cross-env": "^10.1.0",
    "depcheck": "^1.4.7",
    "rimraf": "^6.1.0",
    "ts-unused-exports": "^11.0.1",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3"
  }
}
